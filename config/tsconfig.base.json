{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "noImplicitAny": true,
    "removeComments": true,
    "noLib": false,
    "emitDecoratorMetadata": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "target": "es5",
    "sourceMap": true,
    "lib": ["es6"],
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "pretty": true
  }
}

{
  "extends": "@istanbuljs/nyc-config-typescript",
  "extension": [
    ".ts",
    ".js"
  ],
  "reporter": [
    "text",
    "lcovonly",
    "html"
  ],
  "exclude": "packages/test-utils/src/**",
  "include": "packages/**/src/**",
  "checkCoverage": true,
  "lines": 80,
  "statements": 80,
  "functions": 80,
  "branches": 80
}
